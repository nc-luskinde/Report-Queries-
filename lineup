SELECT distinct
  p.plan_name
  , p.external_id
  , md.description_1 fund
  , md.ticker
  , md.cusip
  , sma.record_keeper_id
  , sma.security_id
  , ps.is_eligible_for_managed_accounts
  , ps.status
  , ps.asset_id
  , md.security_type_id
  , ps.close_date
  , ps.activation_date
 
FROM 
userdb_master.plan_securities ps
left join 
userdb_master.plans p
on p.uid = ps.plan_uid 
join 
MARKET_DATA.ASSETS md 
on md.asset_id = ps.asset_id
left join
market_data.security_master_assets sma
on sma.asset_id  = md.asset_id
-- left join 
-- userdb_master.record_keepers rk
-- on rk.uid = p.record_keeper_uid
where 
upper(p.external_id) = '965118'
--and upper(sma.record_keeper_id) = 'oneamerica'
and sma.security_id like concat(p.external_id, '%')
;
------
SELECT distinct
  p.plan_name
  , p.external_id
  , md.description_1 fund
  , md.ticker
  , ps.is_eligible_for_managed_accounts
  , ps.status
  , ps.asset_id
  , md.security_type_id
  , ps.close_date
  , ps.activation_date
 
FROM 
userdb_master.plan_securities ps
left join 
userdb_master.plans p
on p.uid = ps.plan_uid 
left join 
MARKET_DATA.ASSETS md 
on md.asset_id = ps.asset_id
where 
upper(p.external_id) = 'HMI'
;
