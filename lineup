SELECT distinct
  p.plan_name
  , p.external_id
  , md.description_1 fund
  , md.ticker
  , ps.is_eligible_for_managed_accounts
  , ps.status
  , ps.close_date
  , ps.activation_date
 
FROM 
userdb_master.plan_securities ps
left join 
userdb_master.plans p
on p.uid = ps.plan_uid 
left join 
MARKET_DATA.ASSETS md 
on md.asset_id = ps.asset_id
where 
upper(p.external_id) = 'HMI'
;
---------
SELECT 
userdb_master.plan.plan_name,
userdb_master.plans.external_id,
MARKET_DATA.ASSETS.description_1,
MARKET_DATA.ASSETS.ticker,
userdb_master.plan_securities.activation_date,
userdb_master.plan_securities.close_date
FROM 
userdb_master.plans,
MARKET_DATA.ASSETS, 
userdb_master.plan_securities
where 
MARKET_DATA.ASSETS.asset_id = userdb_master.plan_securities.asset_id 
and 
userdb_master.plan_securities.plan_uid = userdb_master.plans.uid
